{
    "name": "endroid/flusher",
    "description": "Endroid Flusher",
    "keywords": ["endroid", "flusher", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/Flusher",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Flusher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Flusher\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
