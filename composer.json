{
    "name": "endroid/flusher",
    "description": "Endroid Flusher",
    "keywords": ["endroid", "flusher", "php"],
    "homepage": "https://github.com/endroid/flusher",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/orm": "^2.6",
        "endroid/installer": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Flusher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Flusher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
